@model IEnumerable<IGrouping<string, RainbowWine.Services.DO.BrandManufacturerDO>>
@{
    ViewBag.Title = "Brand Manufacturer List";
    var Id = 0;
}

<h2>Brand Manufacturer List</h2>
<h3>
    @Html.ActionLink("Competitor Product List", "ProductCompetitorLinkList", "Orders")
</h3>
<h4>
    @Html.ActionLink("Add New Brand With Manufacturer", "AddBrandManufacturer", "Manufacturer")
</h4>
<table class="table-bordered" width="100%" cellspacing="0">
    <thead class="bg-success">
        <tr>
            <td>
                Manufacturer Name
            </td>

            <td>
                Brand Name
            </td>
            <td>Action</td>
        </tr>
        </thead>
        @foreach (var groupingByClassA in Model)
        {
<tbody>
    <tr>
        <td>
            @groupingByClassA.Key

        </td>
        <td>
            @foreach (var classA in groupingByClassA)
            {
                <table>
                    <tr>
                        <td>
                            @Html.DisplayFor(modelitem => classA.BrandName)

                        </td>
                    </tr>
                </table>
                Id = classA.ManufacturerId;
            }
        </td>
        <td>
            @Html.ActionLink("Edit", "BrandManufacturer", new { id = Id })
            @(" ")
        </td>
    </tr>
</tbody>
        }
</table>
